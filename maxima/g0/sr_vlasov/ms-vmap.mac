load("sr_vlasov/vmap")$
load(stringproc)$
/* This script calls the functions which construct the 
   velocity mapping and inverse Jacobian from a cubic C^1 mapping. */

for v : 1 thru 3 do (
  fname : sconcat("~/max-out/vmap_", v, "v.c"),
  fh : openw(fname),
  funcName : sconcat("vmap_",  v, "v"),
  calc_vmap(fh, funcName, v),
  close(fh)
);
